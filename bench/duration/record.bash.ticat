help = write event duration to meta db, event is a pair of keys, eg: my-event.begin, my-event.end
abbr = rec

[arg]
event-prefix|event = ''

[env]
bench.meta.host = may-read
bench.meta.port = may-read
bench.meta.db-name = may-read
bench.meta.user = may-read
bench.tag = may-read

[dep]
mysql = as client, to write duration data
